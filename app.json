{
  "expo": {
    "name": "React Native TS Starter",
    "slug": "react-native-ts-starter",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "buildNumber": "1",
      "bundleIdentifier": "com.yourname.yourproject",
      "icon": "./assets/icon.png",
      "supportsTablet": false
    },
    "android": {
      "versionCode": 1,
      "package": "com.yourname.yourproject"
    },
    "description": ""
  }
}
